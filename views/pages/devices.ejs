<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>
  <div class="container">
    <a class="btn btn-primary" href="/add-device" role="button">Add Device</a>
    <ul>
      <% devices.forEach(function(device) { %>
        <li><a href="/devices/<%= device.id %>"><%= device.name %> (<%= device.deviceNetworkId %>)</a></li>
          
        <% }); %>
    </ul>
  </div>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>